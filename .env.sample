# Database
DATABASE_URL=postgresql://user:password@localhost:5432/jarvis_mvp
DATABASE_URL_MVP=postgresql://user:password@localhost:5432/jarvis_mvp

# APIs
ELEVENLABS_API_KEY=your_elevenlabs_key
OPENAI_API_KEY=your_openai_key

# Voice Provider Configuration
# Primary provider: 'cartesia' or 'elevenlabs'
TTS_PROVIDER=cartesia

# Cartesia (Primary TTS)
CARTESIA_API_KEY_MVP=
CARTESIA_VOICE_ID_MVP=

# ElevenLabs (Fallback TTS - uses voice_id, NOT Agents Platform)
ELEVENLABS_API_KEY_MVP=
ELEVENLABS_VOICE_ID_MVP=
# DEPRECATED: ELEVENLABS_AGENT_ID_MVP is no longer used

# Mock Mode - Set to 'true' for CI testing without API credits
TTS_MOCK_MODE=false

# Runtime
NODE_ENV=development
LOG_LEVEL=debug
PORT=8080
